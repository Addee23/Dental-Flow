@page
@model DentalFlow.Pages.Services.IndexModel

<h2>Välj behandling</h2>
<hr />

<div class="list-group">
    @foreach (var s in Model.Services)
    {
        <a class="list-group-item list-group-item-action" href="/Bookings/Create/@s.Id">
            @s.Name — @s.DurationMinutes min — @s.Price kr
        </a>
    }
</div>
